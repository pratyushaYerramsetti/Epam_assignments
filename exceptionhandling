import java.util.Scanner;
class MyCalculator {
    /*
    * Create the method long power(int, int) here.
    */
    long power(int n, int p) throws Exception{
        if(n==0 && p==0)
            throw new Exception("n and p should not be zero.");
        if(n<0 || p<0)
            throw new Exception("n or p should not be negative.");  
        long pow = 1;
        int i=0;  
        for(i=1;i<=p;i++)
            pow *= n;
        return(pow);      
    }
    
}
